TABLES: ZCLG.
DATA: IT TYPE STANDARD TABLE OF ZCLG,
      WA TYPE ZCLG.
SELECT-OPTIONS: S_CREGNO FOR ZCLG-CREGNO.
START-OF-SELECTION.
SELECT * FROM ZCLG INTO CORRESPONDING FIELDS OF TABLE IT WHERE CREGNO IN S_CREGNO.
LOOP AT IT INTO WA.
  WRITE: / WA-CREGNO,
           WA-CSNAME,
           WA-CEMAIL,
           WA-CSNAT,
           WA-CADHAR,
           WA-CGENDE,
           WA-CADD,
           WA-CNAT.
  CLEAR WA.
ENDLOOP.
